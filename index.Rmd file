```{r setup, include=FALSE}
knitr::opts_chunk$set(echo = TRUE)
# Create the data frame
patient_data <- data.frame(
  S.NO. = c(101, 102, 103, 104, 105, 106),
  AGE = c(25, 22, 29, 30, 35, 40),
  SEX = c("Male", "Female", "Female", "Male", "Male", "Female"),
  Medical_Status = c("Diabetic", "Non-Diabetic", "Diabetic", 
                    "Non-Diabetic", "Non-Diabetic", "Diabetic")
)

# Display the table
knitr::kable(patient_data, caption = "Patient Medical Data")
# Create the initial data frame
gdp_data <- data.frame(
  Countries = c("Norway", "France", "Mexico", "USA", "Korea"),
  Real_GDP_growth = c(3.22, 2.05, 3.67, 2.99, 5.51),
  Population_Growth = c(0.71, 0.47, 0.65, 1.19, 1.03)
)

# Add the new column
gdp_data$Real_Per_capita_GDP_Growth <- c(2.51, 1.87, 1.54, 1.80, 3.92)

# Display the table
knitr::kable(gdp_data, caption = "Country GDP Data")

